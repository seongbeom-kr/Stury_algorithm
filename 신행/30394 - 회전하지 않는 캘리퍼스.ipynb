   "source": [
    "# 글은 장황한데, 결국에는 y의 최대 - y의 최소\n",
    "\n",
    "# 점의 수\n",
    "n = int(input())\n",
    "\n",
    "max_y = -9999999999999999999\n",
    "min_y = 99999999999999999999\n",
    "# 각 점 좌표\n",
    "for _ in range(n):\n",
    "    x, y = map(int, input().split())\n",
    "    \n",
    "    max_y = max(y, max_y)\n",
    "    min_y = min(y, min_y)\n",
    "    \n",
    "print(max_y - min_y)"
   ]
